
@model IEnumerable<Resume.ViewModels.QualificationViewModel>

@section jsVars{
    <script>
        const controller = "Qualifications";
    </script>
}
<table class="table table-striped table-dark table-hover table-responsive-sm">
    <tbody>
        <tr>
            <td style="padding: 20px;">
                <table>
                    <tbody>
                        <tr>
                            <td style="width: 20vw;">
                                <div class="table-column-header">
                                    <span>Qualifications</span>
                                </div>
                            </td>
                            <td style="width: 80vw;">
                                <div class="table-column-header">
                                    <span>Description</span>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </td>
        </tr>
        <tr>
            <td style="padding: 20px;">
                <table>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td style="width: 20vw;">@item.TechnicalSkillTypeDescription</td>
                            <td style="width: 80vw;">
                                <div>
                                    <span class="paragraph-title" style="padding: 5px;">zoom</span>
                                    @item.TechnicalSkillDescription
                                </div>
                            </td>
                        </tr>
                    }
                </table>
            </td>
        </tr>
    </tbody>
</table>